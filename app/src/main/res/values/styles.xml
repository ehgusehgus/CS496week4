<resources>

    <!-- Base application theme. -->
    <style name="AppTheme" parent="Base.Theme.AppCompat.Light.DarkActionBar">
        <!-- Customize your theme here. -->
        <item name="colorPrimary">@color/colorPrimary</item>
        <item name="colorPrimaryDark">@color/colorPrimaryDark</item>
        <item name="colorAccent">@color/colorAccent</item>
    </style>

    <style name="NoTitle01" parent="Theme.AppCompat.NoActionBar">
        <item name="windowActionBar">false</item>
        <item name="windowNoTitle">true</item>
    </style>

    <style name="SplashTheme" parent="Theme.AppCompat.NoActionBar">
        <item name="android:windowBackground">@drawable/back1</item>
    </style>
    <style name="SplashTheme2" parent="Theme.AppCompat.NoActionBar">
        <item name="android:windowBackground">@drawable/back2</item>
    </style>
    <style name="SplashTheme3" parent="Theme.AppCompat.NoActionBar">
        <item name="android:windowBackground">@drawable/back3</item>
    </style>
    <style name="SplashTheme4" parent="Theme.AppCompat.NoActionBar">
        <item name="android:windowBackground">@drawable/back4</item>
    </style>
    <style name="SplashTheme5" parent="Theme.AppCompat.NoActionBar">
        <item name="android:windowBackground">@drawable/back5</item>
    </style>
</resources>
